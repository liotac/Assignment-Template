// Naive primality testing

var isPrime : boolean = FALSE;
var x : int = 0;
var xfloat : float = 0.0;
var i : int = 2;
var ifloat : float = 2.0;

read x;
xfloat = x;

// Assuming positive x
if !(x == 0 || x == 1)
{
    while i != x || !isPRIME
    {
        isPrime = x / i == xfloat / ifloat;
        i = i + 1;
        ifloat = ifloat + 1;
    }
}
else
{
    isPrime = TRUE;
}

print isPRIME;
